{
    "configurations": [
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Run synology-ds (dev)",
            "runtimeExecutable": "bun",
            "runtimeArgs": [
                "run",
                "src/index.tsx"
            ],
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal"
        },
        {
            "type": "pwa-node",
            "request": "launch",
            "name": "Run synology-ds (built)",
            "program": "${workspaceFolder}/dist/index.js",
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal"
        }
    ]
}
